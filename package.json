  "main": "app/app.js",
  "scripts": {
    "test": "npm run build-watch && NODE_ENV=testing ./node_modules/.bin/mocha './tests/**/*.@(js)' --reporter spec",
    "cover": "npm run build && NODE_ENV=testing ./node_modules/.bin/istanbul cover _mocha -- ./tests/**/*.test.js",
    "build": "npm run clean-public && flow-remove-types app/ -d build/ && gulp build && rm -rf ./build/public && cp -r ./public build/",
    "build-prod": "NODE_ENV=production npm run build",
    "build-watch": "flow-remove-types app/ -d build/ && rm -rf ./build/public && cp -r ./public build/",
    "dev": "npm run build && node build/app.js",
    "dev-watch": "npm run build-watch && node build/app.js",
    "start": "NODE_ENV=production npm run build && NODE_ENV=production node build/app.js",
    "watch": "./node_modules/.bin/nodemon -e js,jsx,less,css,scss,vue -w 'app/**/*.*' -w 'public/**/*.*' -x 'npm run dev-watch'",
    "flow": "flow",
    "clean-public": "rm -rf ./public/front/{css,js,fonts,views}/* && rm -rf ./public/back/{css,js,fonts,views}/*",
    "doc": "./node_modules/.bin/esdoc"
  },
